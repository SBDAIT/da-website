 in state prison.\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"h6\",{children:\"Reporting Hate Crimes to Police\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"p\",{className:\"mt-4\",children:\"If you are a victim of a hate crime, it is important that you report it to law enforcement. To report a hate crime, please call your local law enforcement agency.\"})]}),/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{className:\"secondary-content\",children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{className:\"\\r align-self-center\\r font-bold text-xl font-display \\r border-b-2 border-mustard w-max\",children:\"Contact Information\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"p\",{children:\"\\xA0\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"p\",{children:\"Please contact your local law enforcement agency. \"})]})]})})]});}}]);return HateCrimes;}(react[\"Component\"]);/* harmony default export */ var Prosecution_HateCrimes = (HateCrimes_HateCrimes);\n// CONCATENATED MODULE: ./src/components/pages/Prosecution/DiversionProgram.js\nvar DiversionProgram_DiversionProgram=/*#__PURE__*/function(_Component){Object(inherits[\"a\" /* default */])(DiversionProgram,_Component);var _super=Object(createSuper[\"a\" /* default */])(DiversionProgram);function DiversionProgram(){Object(classCallCheck[\"a\" /* default */])(this,DiversionProgram);return _super.apply(this,arguments);}Object(createClass[\"a\" /* default */])(DiversionProgram,[{key:\"render\",value:function render(){return/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(react[\"Fragment\"],{children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(components_UI_Navbar,{}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{className:\"container\",children:/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{className:\"content-wrapper-no-flex font-display text-base\",style:{backgroundColor:'rgba(112,112,112,0.05)'},children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{className:\"\\r align-self-center\\r font-bol